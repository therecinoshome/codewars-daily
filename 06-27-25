function scoreboard(string) {
  const array = string.toLowerCase().split(' ');
  const score = [];
  const numbers = {
    "one": 1,
    "two": 2,
    "three": 3,
    "four": 4,
    "five": 5,
    "six": 6,
    "seven": 7,
    "eight": 8,
    "nine": 9,
    "nil": 0,
  };
  for(let i = 0; i<array.length; i++) {
    if(numbers[array[i]] !== undefined) score.push(numbers[array[i]])
  }
  return score;
}
